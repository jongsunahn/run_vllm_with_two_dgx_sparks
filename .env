# 컨테이너 이미지 (주신 값)
VLLM_IMAGE=nvcr.io/nvidia/vllm:25.09-py3

# HF 캐시 경로(절대경로)
HF_HOME=/home/eroom/.cache/huggingface

# 헤드 노드 IP (모든 워커가 여기에 붙음)
HEAD_NODE_IP=10.20.0.202

# 이 머신의 실제 IP
# - 헤드 머신: 169.254.36.109
# - 워커 머신: 각 워커의 고유 IP로 설정
VLLM_HOST_IP=10.20.0.202

# 멜라녹스/전용 NIC 이름
MN_IF_NAME=enp1s0f0np0

# Ray 메모리 모니터 (주신 값)
RAY_memory_monitor_refresh_ms=0

MACVLAN_SUBNET=10.20.0.0/24
MACVLAN_GATEWAY=10.20.0.1
